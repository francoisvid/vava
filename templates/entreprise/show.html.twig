{% extends 'base.html.twig' %}

{% block title %}Entreprise{% endblock %}

{% block body %}
    <h1>Entreprise</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entreprise.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ entreprise.nom }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ entreprise.tel }}</td>
            </tr>
            <tr>
                <th>Mail</th>
                <td>{{ entreprise.mail }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ entreprise.logo }}</td>
            </tr>
            <tr>
                <th>Remarque</th>
                <td>{{ entreprise.remarque }}</td>
            </tr>
            <tr>
                <th>SalariesFormes</th>
                <td>{{ entreprise.salariesFormes }}</td>
            </tr>
            <tr>
                <th>SiteWeb</th>
                <td>{{ entreprise.siteWeb }}</td>
            </tr>
            <tr>
                <th>Pub</th>
                <td>{{ entreprise.pub ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Visible</th>
                <td>{{ entreprise.visible ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>IsDeleted</th>
                <td>{{ entreprise.isDeleted ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('entreprise_index') }}">back to list</a>

    <a href="{{ path('entreprise_edit', {'id': entreprise.id}) }}">edit</a>

    {{ include('entreprise/_delete_form.html.twig') }}
{% endblock %}
